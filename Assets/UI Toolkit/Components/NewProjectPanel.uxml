    <Style src="project://database/Assets/UI%20Toolkit/USS/NewProjectPanel.uss?fileID=7433441132597879392&amp;guid=0d28f6331f0365441be9a1c1cc1bdabd&amp;type=3#NewProjectPanel" />
    <engine:VisualElement name="NewProjectRoot" style="flex-grow: 1; background-color: rgb(46, 46, 46); border-top-left-radius: 16px; border-top-right-radius: 16px; border-bottom-right-radius: 16px; border-bottom-left-radius: 16px; padding-top: 24px; padding-right: 24px; padding-bottom: 24px; padding-left: 24px; justify-content: space-between; max-height: 80%;">
        <engine:VisualElement name="ProjectInfoContainer" style="justify-content: space-between; border-bottom-width: 1px; border-left-color: rgb(76, 76, 76); border-right-color: rgb(76, 76, 76); border-top-color: rgb(76, 76, 76); border-bottom-color: rgb(76, 76, 76);">
            <engine:Label text="Create a new project" name="TitleLabel" style="-unity-font-definition: url(&quot;project://database/Assets/UI%20Toolkit/Fonts/Space_Mono/SpaceMono-Bold.ttf?fileID=12800000&amp;guid=98c36e2b9365b974e845c4058050e5fc&amp;type=3#SpaceMono-Bold&quot;); font-size: 20px; -unity-text-align: middle-left; color: rgb(251, 251, 251);" />
            <engine:VisualElement name="ProjectNameContainer" style="justify-content: space-between;">
                <engine:Label text="Project name" name="ProjectNameLabel" style="-unity-font-definition: url(&quot;project://database/Assets/UI%20Toolkit/Fonts/Space_Grotesk/static/SpaceGrotesk-Regular.ttf?fileID=12800000&amp;guid=b29888e73935b11449868e7cb5f7ae4f&amp;type=3#SpaceGrotesk-Regular&quot;); color: rgb(251, 251, 251); -unity-text-align: middle-left;" />
                <engine:Instance template="InputField" name="ProjectNameInputField" style="flex-grow: 0;" />
            </engine:VisualElement>
            <engine:VisualElement name="ProjectDescriptionContainer" style="justify-content: space-between;">
                <engine:Label text="Project description" name="ProjectDescriptionLabel" style="padding-top: 0; padding-right: 0; padding-bottom: 0; padding-left: 0; -unity-font-definition: url(&quot;project://database/Assets/UI%20Toolkit/Fonts/Space_Grotesk/static/SpaceGrotesk-Regular.ttf?fileID=12800000&amp;guid=b29888e73935b11449868e7cb5f7ae4f&amp;type=3#SpaceGrotesk-Regular&quot;); color: rgb(251, 251, 251); -unity-text-align: middle-left;" />
                <engine:Instance template="InputTextField" name="ProjectDescriptionInputField" style="height: 64px; min-height: 64px; max-height: 64px;" />
            </engine:VisualElement>
        </engine:VisualElement>
        <engine:VisualElement name="UploadYourFilesContainer" style="flex-grow: 1; border-bottom-width: 1px; border-left-color: rgb(76, 76, 76); border-right-color: rgb(76, 76, 76); border-top-color: rgb(76, 76, 76); border-bottom-color: rgb(76, 76, 76);">
            <engine:Label text="Upload your files" name="UploadYourFilesLabel" style="color: rgb(251, 251, 251); -unity-font-definition: url(&quot;project://database/Assets/UI%20Toolkit/Fonts/Space_Grotesk/static/SpaceGrotesk-Bold.ttf?fileID=12800000&amp;guid=abd0c980011414e4286a0593c97a0824&amp;type=3#SpaceGrotesk-Bold&quot;); font-size: 16px; -unity-text-align: middle-left;" />
            <engine:VisualElement name="FilesContainer">
                <engine:VisualElement name="AddFilesContainer">
                    <engine:VisualElement name="ContainerTitleChoseFile">
                        <engine:VisualElement name="IconTitleChooseFile" style="flex-grow: 1;" />
                        <engine:VisualElement name="ContainerTextChooseFile" style="flex-grow: 1;">
                            <engine:Label text="Choose a FITS or HDF5 file" name="LabelTitleChooseFile" />
                            <engine:Label text="Files must be FITS or all must be HDF5. Max file size: 5GB" name="LabelDescChooseFile" />
                        </engine:VisualElement>
                    </engine:VisualElement>
                    <engine:Instance template="TertiaryButton" name="AddFileButton">
                        <AttributeOverrides element-name="Label" text="Add files" />
                    </engine:Instance>
                </engine:VisualElement>
                <engine:Label text="Added files&#10;" name="LabelUploadedFiles" />
                <engine:ListView name="ViewFilesContainer" reorderable="true" reorder-mode="Animated" binding-source-selection-mode="Manual" fixed-item-height="41" />
