    border-top-right-radius: 12px;
    border-bottom-right-radius: 12px;
    border-bottom-left-radius: 12px;
    margin-right: -6px;
}

#NewProjectRoot > #UploadYourFilesContainer > #FilesContainer > #ViewFilesContainer > .unity-collection-view__scroll-view {
    flex-grow: 0;
    flex-direction: row;
    flex-wrap: wrap;
    align-items: flex-start;
    justify-content: flex-start;
    flex-shrink: 1;
    overflow: hidden;
    margin-top: 0;
}

#NewProjectRoot > #UploadYourFilesContainer > #FilesContainer > #ViewFilesContainer > .unity-collection-view__scroll-view > #unity-content-and-vertical-scroll-container {
