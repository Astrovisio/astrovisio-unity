    min-width: 24px;
    max-width: 24px;
}

#NewProjectRoot > #UploadYourFilesContainer > #FilesContainer > #AddFilesContainer > #ContainerTitleChoseFile > #ContainerTextChooseFile > #LabelDescChooseFile {
