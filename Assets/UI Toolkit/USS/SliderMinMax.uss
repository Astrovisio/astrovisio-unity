#SliderMinMaxRoot {
    flex-grow: 1;
    justify-content: flex-start;
}

#SliderMinMaxRoot > #Label {
    -unity-font-definition: url("project://database/Assets/UI%20Toolkit/Fonts/Space_Grotesk/static/SpaceGrotesk-Regular.ttf?fileID=12800000&guid=b29888e73935b11449868e7cb5f7ae4f&type=3#SpaceGrotesk-Regular");
    margin-bottom: 0;
    color: var(--grey-700);
}

#SliderMinMaxRoot > .unity-min-max-slider > .unity-min-max-slider__input > .unity-min-max-slider__dragger > .unity-min-max-slider__min-thumb {
    background-image: url("project://database/Assets/UI%20Toolkit/Icons/slider-handle.svg?fileID=7388822144124034973&guid=d431090265ebcb34fa7b14a04cfde810&type=3#slider-handle");
    background-color: var(--transparent);
    height: 24px;
    width: 8px;
    min-width: 8px;
    min-height: 24px;
    max-height: 24px;
    max-width: 8px;
    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 0;
    border-left-width: 0;
    border-top-left-radius: 0;
    border-top-right-radius: 0;
    border-bottom-right-radius: 0;
    border-bottom-left-radius: 0;
    margin-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
    padding-top: 0;
    padding-right: 0;
    padding-bottom: 0;
    padding-left: 0;
    justify-content: center;
    align-items: auto;
    align-content: auto;
    border-left-color: var(--transparent);
    border-right-color: var(--transparent);
    border-top-color: var(--transparent);
    border-bottom-color: var(--transparent);
    -unity-background-image-tint-color: var(--grey-800);
}

#SliderMinMaxRoot > .unity-min-max-slider > .unity-min-max-slider__input > .unity-min-max-slider__dragger > .unity-min-max-slider__max-thumb {
    height: 24px;
    width: 8px;
    min-width: 8px;
    max-width: 8px;
    max-height: 24px;
    min-height: 24px;
    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 0;
    border-left-width: 0;
    border-top-left-radius: 0;
    border-top-right-radius: 0;
    border-bottom-right-radius: 0;
    border-bottom-left-radius: 0;
    background-image: url("project://database/Assets/UI%20Toolkit/Icons/slider-handle.svg?fileID=7388822144124034973&guid=d431090265ebcb34fa7b14a04cfde810&type=3#slider-handle");
    background-color: var(--transparent);
    -unity-background-image-tint-color: var(--grey-800);
    margin-left: 0;
    margin-right: 0;
}

#SliderMinMaxRoot > .unity-min-max-slider > .unity-min-max-slider__input > .unity-min-max-slider__dragger {
    -unity-text-align: middle-left;
    justify-content: center;
    border-top-left-radius: 0;
    border-top-right-radius: 0;
    border-bottom-right-radius: 0;
    border-bottom-left-radius: 0;
    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 0;
    border-left-width: 0;
    border-left-color: var(--transparent);
    border-right-color: var(--transparent);
    border-top-color: var(--transparent);
    border-bottom-color: var(--transparent);
    height: 8px;
    min-height: 8px;
    max-height: 8px;
}

#SliderMinMaxRoot > .unity-min-max-slider > .unity-min-max-slider__input > .unity-min-max-slider__tracker {
    -unity-background-image-tint-color: var(--grey-300);
    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 0;
    border-left-width: 0;
    border-top-left-radius: 3px;
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;
    border-bottom-left-radius: 3px;
    background-color: var(--grey-300);
    margin-left: 0;
    margin-right: 0;
    height: 8px;
    min-height: 8px;
    max-height: 8px;
}

#SliderMinMaxRoot > #MinMaxSlider {
    margin-right: 0;
    margin-left: 0;
    margin-bottom: 5px;
    margin-top: 13px;
}

#SliderMinMaxRoot > #FloatFieldsElement {
    flex-direction: row;
    justify-content: space-between;
}

#SliderMinMaxRoot > #FloatFieldsElement > .min-max-container > .min-max-slider-input {
    border-top-left-radius: 3px;
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;
    border-bottom-left-radius: 3px;
    width: 118px;
    min-width: 118px;
    max-width: 118px;
}

#SliderMinMaxRoot > #FloatFieldsElement > .min-max-container > .min-max-slider-input > #unity-text-input > .unity-text-element--inner-input-field-component {
    -unity-text-align: middle-center;
}

.warning-label {
    height: 14px;
    min-height: 14px;
    max-height: 14px;
    flex-direction: row;
    justify-content: space-around;
    -unity-font-definition: url("project://database/Assets/UI%20Toolkit/Fonts/Space_Mono/SpaceMono-Regular.ttf?fileID=12800000&guid=81aec7828e7531a4fa767fed2a058d81&type=3#SpaceMono-Regular");
    font-size: 10px;
    color: var(--red);
    margin-top: 2px;
}
